{
  "name": "Champlain Fac/Staff Lookup",
  "version": "0.3",
  "description": "Lookup users in the Faculty/Staff Champlain directory",
  "browser_action": {
      "default_title": "Champlain Fac/Staff Lookup",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
   "permissions": [
    "http://*",
    "https://*/"
  ]
}
